<div class="d-flex">
    <div id="sidebar-container" class="bg-warning">
        <div class="menu">
            <a href="vehiculos" class="d-block text-dark h5 p-3"><i class="fas fa-car"></i> Vehículos</a>
            <a href="usuarios" class="d-block text-dark h5 p-3"><i class="fas fa-users"></i> Usuarios</a>
            <a href="eventos" class="d-block text-dark h5 p-3"><i class="fas fa-calendar-alt"></i> Eventos</a>
            <a href="informes" class="d-block text-dark h5 p-3"><i class="fas fa-file"></i> Informes</a>
        </div>
    </div>
    <div id="cont" class="col-md-8 mx-auto mt-3">
        <h3 class="card-title text-center m-2">Informe</h3>
        <hr> 
        <table class="table table-striped bg-warning">
            <thead class="thead-dark">
                <th>Nombre</th><th>Apellido</th><th>Placa vehículo</th><th>Fecha Entrada</th><th>Fecha Salida</th>
            </thead>
            {{#each informes}}
                <tr>
                    <td>{{nombre}}</td>
                    <td>{{apellido}}</td>
                    <td>{{placa}}</td>
                    <td>{{fechaEntrada}}</td>
                    <td>{{fechaSalida}}</td>                    
                </tr>
            {{/each}}       
        </table>
    </div>    
</div>